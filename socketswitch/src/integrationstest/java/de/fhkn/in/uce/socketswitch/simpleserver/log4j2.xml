<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %LOGGER{36} - %msg%n"/>
    </Console>
  </appenders>
  <LOGGERs>
    <root level="all">
      <appender-ref ref="Console"/>
    </root>
  </LOGGERs>
</configuration>